{
  "name": "CQC-Competition-Hub",
  "version": "0.0.0",
  "description": "",
  "repository": "",
  "main": "src/app.js",
  "entrypoint": "index.js",
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.6",
    "browser-cookies": "^1.0.8",
    "classnames": "^2.1.3",
    "date-format": "0.0.2",
    "es6-promise": "^2.0.1",
    "express": "^4.13.4",
    "flux": "^2.0.0",
    "immutable": "^3.8.1",
    "jquery": "^3.0.0",
    "object-assign": "^1.0.0",
    "react": "^15.3.0",
    "react-addons-linked-state-mixin": "^15.4.2",
    "react-addons-pure-render-mixin": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^2.4.1",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "browserify": "^6.3.4",
    "browserify-css": "^0.9.1",
    "chai": "^3.5.0",
    "envify": "^3.4.0",
    "expect": "^1.20.2",
    "jsdom": "^9.4.1",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.2.1",
    "reactify": "^0.15.2",
    "sinon": "^1.17.4",
    "uglify-js": "~2.4.15",
    "watchify": "^2.6.2"
  },
  "scripts": {
    "server": "node server/app.js",
    "watch": "watchify src/app.js -o public/js/bundle.js -v && uglifyjs public/js/bundle.js -o public/js/bundle.min.js",
    "build": "browserify src/app.js -o public/js/bundle.js && uglifyjs public/js/bundle.js -o public/js/bundle.min.js",
    "test": "mocha --require tests/setup.js \"tests/**/*.test.js\" --recursive"
  },
  "browserify-css": {
    "autoInject": false,
    "minify": true,
    "external": [
      "react-dom",
      "react-dom/server",
      "react-addons-test-utils"
    ],
    "minifyOptions": {
      "target": "./public/css/bundle.css"
    },
    "rootDir": "./public/css"
  },
  "author": "stat1c",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/",
      "<rootDir>/node_modules/react-addons-test-utils/"
    ]
  }
}
